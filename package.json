{
  "name": "vimanet-translator",
  "productName": "vimanet-translator",
  "version": "1.0.0",
  "description": "Vimanet translator desktop application powered by electron and angular.",
  "main": "src/main/index.ts",
  "scripts": {
    "postinstall": "bower install",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "lint": "tslint src"
  },
  "keywords": [],
  "author": "Vimanet",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "icon": "src/common/assets/icons/icon.ico",
        "asar": true
      },
      "electronWinstallerConfig": {
        "name": "vimanet-translator"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "vimanet-translator"
      }
    }
  },
  "dependencies": {
    "@angular/common": "^2.4.1",
    "@angular/compiler": "^2.4.1",
    "@angular/core": "^2.4.1",
    "@angular/forms": "^2.4.1",
    "@angular/http": "^2.4.1",
    "@angular/platform-browser": "^2.4.1",
    "@angular/platform-browser-dynamic": "^2.4.1",
    "@angular/router": "^3.4.10",
    "@types/electron": "^1.4.30",
    "angular2-toaster": "^2.0.0",
    "auto-launch": "^5.0.1",
    "block-ui": "^2.70.1",
    "bootstrap": "^3.3.7",
    "devtron": "^1.4.0",
    "electron-compile": "^6.4.1",
    "electron-config": "^1.0.0",
    "electron-devtools-installer": "^2.0.1",
    "electron-is-dev": "^0.1.2",
    "electron-log": "^2.2.6",
    "font-awesome": "^4.7.0",
    "jquery": "^2.2.4",
    "json2csv": "^3.8.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "nedb": "^1.8.0",
    "ng2-bootstrap": "1.2.0",
    "ng2-table": "^1.3.2",
    "ng2-ui-switch": "^1.0.2",
    "reflect-metadata": "^0.1.9",
    "tslib": "^1.4.0",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "bower": "^1.8.0",
